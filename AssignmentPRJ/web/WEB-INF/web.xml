<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller.auth.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlanCreateController</servlet-name>
        <servlet-class>controller.Planning.PlanCreateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailProPlanCreate</servlet-name>
        <servlet-class>controller.Planning.DetailProPlanCreate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlanListController</servlet-name>
        <servlet-class>controller.Planning.PlanListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlanInforController</servlet-name>
        <servlet-class>controller.Planning.PlanInforController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>controller.auth.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HeadPlanning</servlet-name>
        <servlet-class>controller.Planning.HeadPlanning</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OfficerPlanning</servlet-name>
        <servlet-class>controller.Planning.OfficerPlanning</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlanUpdateController</servlet-name>
        <servlet-class>controller.Planning.PlanUpdateController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlanCreateController</servlet-name>
        <url-pattern>/plan/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailProPlanCreate</servlet-name>
        <url-pattern>/productionplan/detail/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlanListController</servlet-name>
        <url-pattern>/plan/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlanInforController</servlet-name>
        <url-pattern>/plan/overview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HeadPlanning</servlet-name>
        <url-pattern>/view/headplanning</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OfficerPlanning</servlet-name>
        <url-pattern>/view/officerplanning</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlanUpdateController</servlet-name>
        <url-pattern>/plan/update</url-pattern>
    </servlet-mapping>
</web-app>
